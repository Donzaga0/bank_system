    <script>
        // Page navigation function
        function showPage(pageId) {
            // Hide all pages
            document.querySelectorAll('.page').forEach(page => {
                page.classList.remove('active');
            });
            
            // Show selected page
            document.getElementById(pageId + '-page').classList.add('active');
            
            // Update navigation active state
            document.querySelectorAll('.nav-item').forEach(item => {
                item.classList.remove('active');
            });
            
            // Set active nav item
            if (pageId === 'dashboard') {
                document.querySelectorAll('.nav-item')[0].classList.add('active');
            } else if (pageId === 'airdrop') {
                document.querySelectorAll('.nav-item')[1].classList.add('active');
            } else if (pageId === 'star') {
                document.querySelectorAll('.nav-item')[2].classList.add('active');
            }
            
            // Scroll to top when changing pages
            window.scrollTo(0, 0);
        }
        
        // Wallet connection simulation
        document.getElementById('connectWallet').addEventListener('click', function() {
            const walletAddress = document.getElementById('walletAddress');
            if (walletAddress.textContent === 'Not Connected') {
                walletAddress.textContent = 'EQD...XyZ';
                this.innerHTML = '<i class="bi bi-wallet2"></i>';
                this.classList.add('btn-success');
                this.classList.remove('btn-primary');
            } else {
                walletAddress.textContent = 'Not Connected';
                this.innerHTML = '<i class="bi bi-wallet2"></i>';
                this.classList.add('btn-primary');
                this.classList.remove('btn-success');
            }
        });
        
        // Simulate TON price updates
        function updateTonPrice() {
            const prices = [2.95, 2.96, 2.94, 2.97, 2.93];
            const randomPrice = prices[Math.floor(Math.random() * prices.length)];
            document.getElementById('tonPrice').textContent = `$${randomPrice}`;
        }
        
        // Update price every 10 seconds
        setInterval(updateTonPrice, 10000);
        
        // Add touch feedback to interactive elements
        document.querySelectorAll('.card-custom, .btn, .nav-item').forEach(element => {
            element.addEventListener('touchstart', function() {
                this.style.opacity = '0.7';
            });
            
            element.addEventListener('touchend', function() {
                this.style.opacity = '1';
            });
        });
        
        // Prevent zoom on double tap
        let lastTouchEnd = 0;
        document.addEventListener('touchend', function (event) {
            const now = (new Date()).getTime();
            if (now - lastTouchEnd <= 300) {
                event.preventDefault();
            }
            lastTouchEnd = now;
        }, false);
    </script>
</body>
</html>